    submit.addEventListener("click", function (e) {
	console.log("Button Clicked");
	console.log(search.value);
	news.innerHTML = "asd";
	e.preventDefault;
	const data = fetch(
		"https://newsapi.org/v2/top-headlines?country=id&page=1&apiKey=1fb6ffde575e427b8befadc1a76de898",
	);
	data
		.then(function (res) {
			return res.json();
		})
		.then((res) => {
			news.innerHTML = render(res);
		})
		.catch((err) => {
			news.innerHTML = message(err.message);
		})
		.finally(() => {});
});
news.innerHTML = message("loading...");

function render(result) {
	let news = "";
	let asd = result.articles;
	asd.forEach((data) => {
		news += `<div class="col">
	        <div class="card">
	          <img src="${data.urlToImage}" class="card-img-top" alt="...">
	          <div class="card-body">
	            <h5 class="card-title">${data.title}</h5>
                <h6 class="text-muted">${data.author} | ${data.publishedAt}</h6>
	            <p class="card-text">${data.description}</p>
	            <a href="#" class="btn btn-primary">Baca Detail</a>
	          </div>
	        </div>
	      </div>`;
	});
	return news;
}

function message(msg) {
	return `<div class="col">
            <div class="card">
              <img src="https://images.unsplash.com/photo-1438761681033-6461ffad8d80?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=870&q=80" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title">Stil ERRORR</h5>
                <p class="card-text">${msg}</a>
              </div>
            </div>
          </div>`;
}